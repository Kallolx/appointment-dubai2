# This server uses nginx, not Apache
# For nginx configuration, see nginx.conf file
# If you need to force nginx to read this, contact your hosting provider

# Some shared hosting providers read these nginx directives from .htaccess
location / {
    try_files $uri $uri/ /index.html;
}

# Cache static assets
location ~* \.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$ {
    expires 1y;
    add_header Cache-Control "public, immutable";
}
